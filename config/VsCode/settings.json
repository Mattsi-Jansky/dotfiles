{
    "window.zoomLevel": 1,
    "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
    "workbench.iconTheme": "vscode-icons",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.tabSize": 2,
    "FSharp.fsiFilePath": "/usr/local/bin/dotnet",
